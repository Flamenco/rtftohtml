{
  "name": "@charles-owen/rtftohtml",
  "version": "1.0.5",
  "description": "RTF to HTML converter",
  "main": "dist/rtftohtml.min.js",
  "module": "index.js",
  "private": false,
  "scripts": {
    "build:dev": "npx webpack --config webpack.dev.js",
    "build:prod": "npx webpack --config webpack.prod.js",
    "build:all": "npm run build:dev && npm run build:prod",
    "test": "karma start karma.conf.js",
    "serve": "webpack-dev-server  --config webpack.dev.js --open",
    "build:docs": "documentation build src/Cirsim/**/*.js --sort-order source -f html -o doc/js"
  },
  "keywords": [
    "RTF",
    "HTML"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charles-owen/rtftohtml.git"
  },
  "author": "Charles B. Owen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/runtime": "7.12.13",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.1.0",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "4.0.0-beta.0",
    "webpack-merge": "^5.7.3",
    "jasmine-core": "^3.6.0",
    "karma": "^6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "clean-webpack-plugin": "^3.0.0"
  }
}
